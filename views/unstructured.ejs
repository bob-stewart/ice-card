      function nest(o) {
        %><ul><%
        for (var key in o) {
          %><li><%- key %><br /><%
          var val = o[key];
          if (typeof(val) == typeof({})) {
            nest(val);
          }
          else {
            %><%- val %><%
          }
          %></li><%
        }
        %></ul><%
      }
      nest(data);
